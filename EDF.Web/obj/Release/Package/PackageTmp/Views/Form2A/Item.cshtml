                                        <div class=" col-md-8 mt-1">
                                            <input type="number" class="form-control others" id="others" value="@Form2_A_AutoFillData[i].others" placeholder="">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-md-4 mt-3">
                                            <label for="">
                                                Total:<span class="text-danger">*</span>
                                            </label>
                                        </div>
                                        <div class="form-group col-md-8 mt-3">
                                            <input type="number" class="form-control total" id="total" placeholder="Total" value="@Form2_A_AutoFillData[i].total" readonly>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class=" col-md-4 mt-1">
                                            <label for="">Annual Income from Property:</label>
                                        </div>
                                        <div class=" col-md-8 mt-1">

                                            <input type="number" class="form-control" id="annualIncomeFromProperty" value="@Form2_A_AutoFillData[i].annualIncomeFromProperty" placeholder="">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class=" col-md-4 mt-3">

                                            <label for="">Remarks</label>
