        </form>
    </div>
</div>


<div class="col-md-12" style="text-align:right;">
    <button type="button" class="btn-primary mt-3" id="addRowbtn" onclick="addRow()">+</button>
</div>

<div class="row">

    <div class="col-md-12" style="text-align:center">
        <button class="btn btn-primary" id="submitBtn">Submit</button>
        <a href="Form_5.html" class="btn btn-primary">Cancel</a>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content modal-lg">
            <div class="modal-header" align="center">
                <h4 class="modal-title" id="exampleModalLabel"><b>Sold Property Details</b></h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="float:right;">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6 d-none">
                        <label for=""> Employee Code <span class="text-danger">*</span></label>
                        <input type="text" class="form-control" id="employeeid" placeholder="" value="@UserManager.User.employeecode" readonly>
                    </div>
                    <div class="form-group col-md-4">
                        <label for="">Sold To</label>
                        <input type="text" class="form-control" id="soldTo" placeholder="Enter Name">
                    </div>
                    <div class="form-group col-md-4">
                        <label for="">Sold Amount</label>
                        <input type="number" class="form-control" id="soldAmount" placeholder="Enter Value">
                    </div>
                    <div class="form-group col-md-4">
                        <label for="">Sold Date</label>
                        <input type="date" class="form-control" id="soldDate" placeholder="Enter Date">
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>

                <button type="button" class="btn btn-primary" onclick="saveSoldPropData()">Save changes</button>
            </div>
        </div>
    </div>
</div>



<script>

    var countr = parseInt('@counter') || 0;

    $('body').on('change', '.propownedby', function () {

        $(".propownedby").each(function () {
            var nameOfSpouseDiv = $(this).parent().parent().find(".nameofspouse_parentc");

            var selectedValue = $(this).find("option:selected").text();;

            if (selectedValue.trim() === 'Self') {
                nameOfSpouseDiv.hide(); // Hide input field
            } else {
                nameOfSpouseDiv.show(); // Show input field
            }
        })
    });



    function addRow() {

        var html = $(".mainDiv1").html();
        $("#childDivForm").append(html);

        $('body').on('change', '.propownedby', function () {

            $(".propownedby").each(function () {

                var selectedValue = $(this).find("option:selected").text();;
                var nameOfSpouseDiv = $(this).parent().parent().find(".nameofspouse_parentc");

                if (selectedValue.trim() === 'Self') {
                    nameOfSpouseDiv.hide(); // Hide input field
                } else {
                    nameOfSpouseDiv.show(); // Show input field
                }
            })
        });


        $("input").keyup(function () {
            $(".calculation").each(function () {

                var total = 0;
                var housingLoan = parseInt($(this).find("#housingLoan").val()) || 0;
                var saving = parseInt($(this).find("#saving").val()) || 0;
                var pfLoan = parseInt($(this).find("#pfLoan").val()) || 0;
                var others = parseInt($(this).find("#others").val()) || 0;

                total = housingLoan + saving + pfLoan + others;

                $(this).find("#total").val(total);
            })

        });
    }


    function toggleDiv() {
        var notApplicable = $('#notApplicableCheckbox').is(":checked");
        if (notApplicable) {
            $(".propertybox").hide();
        } else {
            $(".propertybox").show();
        }
    }

    document.querySelectorAll("#housingLoan, #saving, #pfLoan, #others, #annualIncomeFromProperty, #presentMarketValue, #soldAmount").forEach(function (element) {
        element.addEventListener("keypress", function (evt) {
            if (evt.which != 8 && evt.which != 0 && (evt.which < 48 || evt.which > 57)) {
                evt.preventDefault();
            }
        });
    });




    $("input").keyup(function () {
        $(".calculation").each(function () {

            var total = 0;
            var housingLoan = parseInt($(this).find("#housingLoan").val()) || 0;
            var saving = parseInt($(this).find("#saving").val()) || 0;
            var pfLoan = parseInt($(this).find("#pfLoan").val()) || 0;
            var others = parseInt($(this).find("#others").val()) || 0;

            total = housingLoan + saving + pfLoan + others;

            $(this).find("#total").val(total);
        })

    });


    var soldPropertyDetails = [];

    var closestRemovebtn;
    var hdnPropertyId;
    function saveSoldPropData() {

        var employeecode = $("#employeeid").val();

        var soldTo = $("#soldTo").val().trim();
        var soldAmount = $("#soldAmount").val().trim();
        var soldDate = $("#soldDate").val().trim();

        if (soldTo === "") {
            alert("Sold To field is required");
            return;
        }

        if (soldAmount === "") {
            alert("Sold Amount field is required");
            return;
        }

        if (soldDate === "") {
            alert("Sold Date field is required");
            return;
        }

        var currentDate = new Date();
        if (new Date(soldDate) > currentDate) {
            alert("Sold Date can not be future date !");
            return;
        }


        var soldDetails = {
            property_id: hdnPropertyId,
            employeecode: employeecode,
            sold_to: soldTo,
            sold_amt: soldAmount,
            sold_date: soldDate,
        };

        soldPropertyDetails.push(soldDetails);

        $(closestRemovebtn).closest('.innerDiv').remove();
        $("#exampleModal").hide();

        // Remove the backdrop overlay
        $('body').removeClass('modal-open');
        $('.modal-backdrop').remove();

        //if (countr == soldPropertyDetails.length) {
        //    $('#notApplicableCheckbox').removeAttr('disabled');
        //    $('#notApplicableCheckbox').prop('checked', true);
        //    $('#addRowbtn').attr('disabled', 'disabled');
        //}
        //else {
        //    $('#notApplicableCheckbox').removeAttr("checked");
        //}
    }

    function removeRow(element) {
        debugger;
        var isshowPop = parseInt($(element).data("isshowpopup")) || 0;
        if (isshowPop > 0 && isshowPop != undefined) {
            $("#soldDate").val("");
            $("#soldAmount").val("");
            $("#soldTo").val("");
            $("#exampleModal").show();
            $(element).attr("data-toggle", "modal").attr("data-target", "#exampleModal");
            hdnPropertyId = $(element).data('property_id');
            console.log(hdnPropertyId);
        }
        else {
            $(element).closest('.innerDiv').remove();
        }
        closestRemovebtn = element;
    }
    //console.log(soldPropertyDetails);

    $("#submitBtn").click(function (event) {
        event.preventDefault();
        debugger;
        $(".validation-message").remove();
        var notApplicable = $("#notApplicableCheckbox").prop("checked");
        var employeecode = $("#employeeid").val();

        if (notApplicable) {
            var allDetails = {
                'employeecode': employeecode,
                'not_applicable': notApplicable,
                'soldPropertyDetails': soldPropertyDetails
            }
            submitForm(allDetails);
        } else {

            validateAndSubmitForm();
        }
    });

    function validateAndSubmitForm() {

        var error = false;
        var employeecode = $("#employeeid").val();

        var notApplicable = $("#notApplicableCheckbox").prop("checked")

        // Validate fields inside propertybox
        $("#childDivForm .innerDiv, .mainDiv .innerDiv, .childDiv1 .innerDiv").each(function () {

            var propertybox = $(this);
            //  var employeecode = $("#employeeid").val();
            var natureOfProperty = propertybox.find("#natureOfProperty").val();
            var propertyOwnedBy = propertybox.find("#propertyOwnedBy").val();
            var presentMarketValue = propertybox.find("#presentMarketValue").val();
            var nameOfSpouse = propertybox.find("#nameOfSpouse").val();
            var natureOfAcuisition = propertybox.find("#natureOfAcuisition").val();
            var dateOfAcuisition = propertybox.find("#dateOfAcuisition").val();
            var state = propertybox.find("#state").val();
            var city = propertybox.find("#city").val();
            var pinCode = propertybox.find("#pinCode").val();
            var streetAddress = propertybox.find("#streetAddress").val();
            var acquiredName = propertybox.find("#acquiredName").val();
            var acquiredAddress = propertybox.find("#acquiredAddress").val();


            var housingLoan = propertybox.find("#housingLoan").val();
            var pfLoan = propertybox.find("#pfLoan").val();
            var saving = propertybox.find("#saving").val();
            var others = propertybox.find("#others").val();
            var total = propertybox.find("#total").val();
            var annualIncomeFromProperty = propertybox.find("#annualIncomeFromProperty").val();
            var remarks = propertybox.find("#remarks").val();


            if (natureOfProperty == "") {
                propertybox.find("#natureOfProperty").focus();
                alert("Nature Of Property is required");
                error = true;
                return;
            }

            if (propertyOwnedBy == "") {
                propertybox.find("#propertyOwnedBy").focus();
                alert("Property owned is required");
                error = true;
                return;
            }
            if (presentMarketValue === "") {
                propertybox.find("#presentMarketValue").focus();
                alert("Present Market Value is required");
                error = true;
                return false;
            }
            if (nameOfSpouse == "" && propertyOwnedBy !== "10") {
                propertybox.find("#nameOfSpouse").focus();
                alert("Name of Spouse/Parents is required");
                error = true;
                return;
            }

            if (natureOfAcuisition == "") {
                propertybox.find("#natureOfAcuisition").focus();
                alert("Nature Of Acuisition is required");
                error = true;
                return;
            }

            if (dateOfAcuisition == "") {
                propertybox.find("#dateOfAcuisition").focus();
                alert("Date Of Acuisition is required");
                error = true;
                return;
            }
            var currentDate = new Date();

            if (new Date(dateOfAcuisition) > currentDate) {
                propertybox.find("#dateOfAcuisition").focus();
                alert("Date Of Acuisition can not be future date !");
                error = true;
                return;
            }
            if (streetAddress == "") {
                propertybox.find("#streetAddress").focus();
                alert("Street Address is required");
                error = true;
                return;
            }
            if (city == "") {
                propertybox.find("#city").focus();
                alert("City is required");
                error = true;
                return;
            }
            if (pinCode == "") {
                alert("Pin Code is required");
                propertybox.find("#pinCode").focus();
                error = true;
                return;
            }
            else if (pinCode.length > 6 || pinCode.length < 6) {
                alert("Pin code must be 6 digit");
                propertybox.find("#pinCode").focus();
                error = true;
                return;
            }
            if (state == "") {
                propertybox.find("#state").focus();
                alert("State is required");
                error = true;
                return;
            }
            if (acquiredName == "") {
                propertybox.find("#acquiredName").focus();
                alert("Acquired Name is required");
                error = true;
                return;
            }
            if (acquiredAddress == "") {
                propertybox.find("#acquiredAddress").focus();
                alert("Acquired Address is required");
                error = true;
                return;
            }

            if (housingLoan == "") {
                propertybox.find("#housingLoan").focus();
                alert("Housing loan is required");
                error = true;
                return;
            }
            if (pfLoan == "") {
                propertybox.find("#pfLoan").focus();
                alert("PF Loan loan is required");
                error = true;
                return;
            }
            if (saving == "") {
                propertybox.find("#saving").focus();
                alert("Saving Loan loan is required");
                error = true;
                return;
            }
            //if (others == "") {
            //    alert("Others Sources of funds required");
            //    error = true;
            //    return;
            //}
            if (total == "") {
                propertybox.find("#total").focus();
                alert("Total Sources of funds required");
                error = true;
                return;
            }
            //if (annualIncomeFromProperty == "") {
            //    alert("Annual Income from Property is required");
            //    error = true;
            //    return;
            //}

            //if (remarks == "") {
            //  alert("Remarks is required");
            //error = true;
            //return;
            //}

        });

        if (!error) {

            var formTwoModel = [];
            $("#childDivForm .innerDiv, .mainDiv .innerDiv, .childDiv1 .innerDiv").each(function () {
                var formTwoEntry = {};
                var propertybox = $(this);
                propertybox.find('input, select, textarea').each(function () {

                    var inputField = $(this);
                    var fieldName = inputField.attr('id');
                    var fieldValue = inputField.val();
                    formTwoEntry[fieldName] = fieldValue;
                });
                formTwoModel.push(formTwoEntry);
            });

            var allDetails = { 'not_applicable': notApplicable, 'formTwoModel': formTwoModel, 'soldPropertyDetails': soldPropertyDetails };

            submitForm(allDetails);
        }
    }

    function submitForm(allDetails) {
        $.ajax({
            url: "/Form2A/AddFormtwoData",
            type: "POST",
            data: allDetails,
            success: function (result) {
                if (result.Status) {
                    alert(result.Message);
                    window.location.href = "/Form2A";
                } else {
                    alert(result.Message);
                    console.error(result.errors);
                }
            },
            error: function (err) {
                alert('An error occurred while saving data.');
            }
        });
    }
</script>
