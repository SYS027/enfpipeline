
@{
    ViewBag.Title = "AuditReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header><h4><b>Audit Report</b></h4></header>
<hr>
<div class="table-responsive mt-5" style="border: 1px solid #ccc; padding: 1%;">
    <table class="table" id="auditreport" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th scope="col">S.No.</th>
                <th scope="col">Employee Code</th>
                <th scope="col">Employee Name</th>
                <th scope="col">Nomination Type</th>
                <th scope="col">Family Member Name</th>
                <th scope="col">Old Share of Percentage</th>
                <th scope="col">New Share of Percentage</th>
                @*<th scope="col">Family Member Name</th>
                <th scope="col">Relation With Employee</th>*@
            </tr>
        </thead>

        <tbody>
        </tbody>

    </table>
</div>

